  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.20.0",
    "@types/node": "^20.14.9",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fern-api": "^0.60.30",
    "prettier": "^3.3.2",
